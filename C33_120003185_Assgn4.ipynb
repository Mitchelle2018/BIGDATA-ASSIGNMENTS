def findAvg(s1):
    avg=0;
    for i in range(len(s1)):
        avg+=s1[i]
    avg=avg/len(s1)
    return avg

def repList(s1):
    l=list()
    for i in s1:
        if(i is not 0):
            l.append(i-findAvg(s1))
        else:
            l.append(0)
    return l

