def findAvg(s1):
    avg=0;
    for i in range(len(s1)):
        avg+=s1[i]
    avg=avg/len(s1)
    return avg

def findRatings(s1,s2):
    l=[0]*18
    m=list(s2)
    rind=0
    for i in s1:
        pos=0
        j=m[rind]*genre_values[i]
        j=j[5:]
        for ind in j:
            l[pos]+=ind
            pos+=1
        rind+=1
            
            
    
    avg=findAvg(l)

    for i in range(18):
        l[i]/=avg
        l[i]=round(l[i],2)
    return l

